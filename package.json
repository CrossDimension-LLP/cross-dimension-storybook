{
  "name": "xd-storybook",
  "version": "1.0.0-alpha",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.0-alpha.57",
    "@storybook/addon-essentials": "^7.0.0-alpha.57",
    "@storybook/addon-interactions": "^7.0.0-alpha.57",
    "@storybook/addon-links": "^7.0.0-alpha.57",
    "@storybook/builder-webpack5": "^7.0.0-alpha.57",
    "@storybook/manager-webpack5": "^7.0.0-alpha.10",
    "@storybook/node-logger": "^7.0.0-alpha.57",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^7.0.0-alpha.57",
    "@storybook/testing-library": "^0.0.14-next.0",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "6.22.0",
    "chromatic": "^6.11.4",
    "prop-types": "^15.8.1",
    "webpack": "^5.75.0"
  }
}
